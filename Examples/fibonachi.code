funct fibo ( x ) {
    if ( x == 0 ) {
        return 0 $
    }
    
    if ( x == 1 ) {
        return 1 $
    }

    if ( x == 2 ) {
        return 1 $
    }

    var result = 0 $

    x = x - 1 $
    result = fibo ( x ) $

    x = x - 1 $
    result = result + fibo ( x ) $


   return result $
} /* fibonachi */

main {   /* main */
   var x = 0 $
   var y = 0 $

   in x $

   y = fibo ( x ) $

   out y $
}
